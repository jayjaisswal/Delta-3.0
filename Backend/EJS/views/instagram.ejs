<body>
    <h2>This page belong to <%= data.name %></h2>
    <button>Follow</button>
    <button>Message</button>

    <p>
        Followers: <%= data.followers %> &nbsp; &nbsp;&nbsp;&nbsp; Following : <%=
        data.following%>
    </p>
    <hr>

    <% for(let post of data.posts) { %>
        <img src = "<%= post.image %>" alt="some img" />
        <p>
            Likes : <%= post.likes %> &nbsp; &nbsp;&nbsp;&nbsp; Comments : <%=
        post.comments%>
        </p>
   <% } %>
    
   <%- include("includes/footer .ejs") %>
</body>
</html>
